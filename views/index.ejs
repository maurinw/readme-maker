<div
  class="editor"
  id="editor"
  data-readme-components="<%= typeof readme !== 'undefined' && readme && readme.components ? JSON.stringify(readme.components) : '[]' %>"
>
  <div class="card components">
    <div class="card-header">
      <h2>Components</h2>
    </div>
    <div class="card-body">
      <h3 class="card-sub-header">Selected Components</h3>
      <ul id="selectedList"></ul>

      <h3 class="card-sub-header">Available Components</h3>
      <ul id="availableList"></ul>
    </div>
  </div>

  <div class="card edit">
    <div class="card-header">
      <h2>Editor</h2>
    </div>
    <div class="card-body">
      <textarea
        id="editorTextarea"
        placeholder="Select a component to edit..."
      ></textarea>
    </div>
  </div>

  <div class="card preview">
    <div class="card-header">
      <h2>Preview</h2>
    </div>
    <div class="card-body">
      <div id="previewArea">
        <p>Preview will be displayed here.</p>
      </div>
    </div>
  </div>
</div>
